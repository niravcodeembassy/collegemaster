{
    "name": "aj/multi-auth",
    "description": "Laravel Multi Authentication",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "multi",
        "authentication"
    ],
    "authors": [
        {
            "name": "Ajay Gadhiya",
            "email": "ajaypatel99094@gmail.com",
            "role": "Developer"
        }
    ],
 
    "require": {
        "php": "^7.2.5",
        "illuminate/auth": "^7.0",
        "illuminate/console": "^7.0",
        "illuminate/notifications": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Aj\\MultiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aj\\MultiAuth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Aj\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    }
}
